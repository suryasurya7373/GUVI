import java.util.*;
public class Random_Program {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		String s=sc.next();		
		Random r=new Random();
		for(int i=0;i<n;i++){
			int m=r.nextInt(10);
			System.out.print(m+" ");
		}	
		for(int i=0;i<n;i++){
			char m=s.charAt(r.nextInt(s.length()));
			System.out.print(m+" ");
		}		
	}
}
